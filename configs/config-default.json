{
  "LOG": true,
  "LOG_LEVEL": "debug",
  "HOST": "0.0.0.0",
  "PORT": 3000,
  "API_TIMEOUT_MS": "600000",
  "PROXY_URL": "http://127.0.0.1:7890",
  "providers": [
    {
      "id": "openai",
      "name": "openai",
      "type": "openai",
      "api_base_url": "https://api.openai.com/v1/chat/completions",
      "api_key": "${OPENAI_API_KEY}",
      "models": [
        "gpt-4o",
        "gpt-5",
        "gpt-5-pro",
        "gpt-5.1",
        "gpt-5.1-pro"
      ],
      "transformer": {
        "use": ["openai"],
        "gpt-4o": {
          "use": ["openai", "openai-responses"]
        },
        "gpt-5": {
          "use": ["openai", "openai-responses"]
        },
        "gpt-5-pro": {
          "use": ["openai", "openai-responses"]
        },
        "gpt-5.1": {
          "use": ["openai", "openai-responses"]
        },
        "gpt-5.1-pro": {
          "use": ["openai", "openai-responses"]
        }
      }
    },
    {
      "id": "openrouter",
      "name": "openrouter",
      "type": "openai",
      "api_base_url": "https://openrouter.ai/api/v1/chat/completions",
      "api_key": "${OPENROUTER_API_KEY}",
      "models": [
        "x-ai/grok-code-fast-1",
        "x-ai/grok-4-fast",
        "anthropic/claude-sonnet-4.5",
        "anthropic/claude-haiku-4.5",
        "anthropic/claude-opus-4.1",
        "google/gemini-2.5-flash",
        "google/gemini-2.5-pro",
        "minimax/minimax-m2",
        "openai/gpt-5",
        "openai/gpt-5-pro",
        "openai/gpt-5-mini",
        "openai/gpt-5.1",
        "openai/gpt-5.1-chat",
        "openai/gpt-5.1-codex",
        "openai/gpt-5-image"
      ],
      "transformer": {
        "use": ["openrouter"]
      }
    }
  ],
  "Router": {
    "default": "openrouter,anthropic/claude-sonnet-4.5"
  }
}

